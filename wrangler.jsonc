{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "portuguese-ai-app",
	"main": "server/index.tsx",
	"compatibility_date": "2025-12-25",
	"compatibility_flags": ["nodejs_compat"],
	"assets": {
		"binding": "ASSETS",
		"directory": "./public"
	},
	"observability": {
		"enabled": true
	},
	"d1_databases": [
    {
      "binding": "DB",
      "database_name": "portuguese-ai",
      "database_id": "6af69982-1208-4d5a-a556-6609d2ad10a0"
    }
  ]
